@using TourTravelDemo.Models
@model CategoryBlog
@{
    ViewBag.Title = "Delete";
}
@section style{
    <!-- Favicon -->
    <link rel="icon" href="~/Areas/Admin/assets/images/brand/favicon.ico" type="image/x-icon" />
    <link rel="shortcut icon" type="image/x-icon" href="~/Areas/Admin/assets/images/brand/favicon.ico" />

    <!-- Title -->
    <title>Hogo – Creative Admin Multipurpose Responsive Bootstrap4 Dashboard HTML Template</title>

    <!--Bootstrap.min css-->
    <link rel="stylesheet" href="~/Areas/Admin/assets/plugins/bootstrap/css/bootstrap.min.css">

    <!-- Dashboard css -->
    <link href="~/Areas/Admin/assets/css/style.css" rel="stylesheet" />

    <!-- Custom scroll bar css-->
    <link href="~/Areas/Admin/assets/plugins/scroll-bar/jquery.mCustomScrollbar.css" rel="stylesheet" />

    <!-- Sidemenu css -->
    <link href="~/Areas/Admin/assets/plugins/toggle-sidebar/full-sidemenu-dark.css" rel="stylesheet" />

    <!--Daterangepicker css-->
    <link href="~/Areas/Admin/assets/plugins/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet" />

    <!-- Rightsidebar css -->
    <link href="~/Areas/Admin/assets/plugins/sidebar/sidebar.css" rel="stylesheet">

    <!-- Sidebar Accordions css -->
    <link href="~/Areas/Admin/assets/plugins/accordion1/css/easy-responsive-tabs.css" rel="stylesheet">

    <!-- Owl Theme css-->
    <link href="~/Areas/Admin/assets/plugins/owl-carousel/owl.carousel.css" rel="stylesheet">

    <!-- Morris  Charts css-->
    <link href="~/Areas/Admin/assets/plugins/morris/morris.css" rel="stylesheet" />

    <!---Font icons css-->
    <link href="~/Areas/Admin/assets/plugins/iconfonts/plugin.css" rel="stylesheet" />
    <link href="~/Areas/Admin/assets/plugins/iconfonts/icons.css" rel="stylesheet" />
    <link href="~/Areas/Admin/assets/fonts/fonts/font-awesome.min.css" rel="stylesheet">
}
<!-- page-header -->
<div class="page-header">
    <ol class="breadcrumb">
        <!-- breadcrumb -->
        <li class="breadcrumb-item"><a href="#">Tables</a></li>
        <li class="breadcrumb-item active" aria-current="page">Default Table</li>
    </ol><!-- End breadcrumb -->
    <div class="ml-auto">
        <div class="input-group">
            <a class="btn btn-primary text-white mr-2" id="daterange-btn">
                <span>
                    <i class="fa fa-calendar"></i> Events Settings
                </span>
                <i class="fa fa-caret-down"></i>
            </a>
            <a href="#" class="btn btn-secondary text-white" data-toggle="tooltip" title="" data-placement="bottom" data-original-title="Rating">
                <span>
                    <i class="fa fa-star"></i>
                </span>
            </a>
        </div>
    </div>
</div>
<!-- End page-header -->

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header">
                <h3 class="card-title">Delete Category</h3>
            </div>
            <div class="card-bordy">
                <p>@Html.DisplayNameFor(p => p.Id) : @Html.DisplayFor(p => p.Id)</p>
                <p>@Html.DisplayNameFor(p => p.Title) : @Html.DisplayFor(p => p.Title)</p>
                <p>@Html.DisplayNameFor(p => p.CreateDate) : @Html.DisplayFor(p => p.CreateDate)</p>
                <p>@Html.DisplayNameFor(p => p.ModifiedDate) : @Html.DisplayFor(p => p.ModifiedDate)</p>
                <p>
                    <a href="@Url.Action("Index")" class="btn btn-primary text-white mr-2">Quay lại</a>
                    <a href="@Url.Action("DeleteConfirm",new { id = Model.Id })" class="btn btn-danger">Xóa</a>

                </p>


            </div>
        </div>
    </div>
</div>

@section scripts{
    <!-- Jquery js-->
    <script src="~/Areas/Admin/assets/js/vendors/jquery-3.2.1.min.js"></script>

    <!--Bootstrap.min js-->
    <script src="~/Areas/Admin/assets/plugins/bootstrap/popper.min.js"></script>
    <script src="~/Areas/Admin/assets/plugins/bootstrap/js/bootstrap.min.js"></script>

    <!--Jquery Sparkline js-->
    <script src="~/Areas/Admin/assets/js/vendors/jquery.sparkline.min.js"></script>

    <!-- Chart Circle js-->
    <script src="~/Areas/Admin/assets/js/vendors/circle-progress.min.js"></script>

    <!-- Star Rating js-->
    <script src="~/Areas/Admin/assets/plugins/rating/jquery.rating-stars.js"></script>

    <!--Moment js-->
    <script src="~/Areas/Admin/assets/plugins/moment/moment.min.js"></script>

    <!-- Daterangepicker js-->
    <script src="~/Areas/Admin/assets/plugins/bootstrap-daterangepicker/daterangepicker.js"></script>

    <!--Side-menu js-->
    <script src="~/Areas/Admin/assets/plugins/toggle-sidebar/sidemenu.js"></script>

    <!-- Sidebar Accordions js -->
    <script src="~/Areas/Admin/assets/plugins/accordion1/js/easyResponsiveTabs.js"></script>

    <!-- Custom scroll bar js-->
    <script src="~/Areas/Admin/assets/plugins/scroll-bar/jquery.mCustomScrollbar.concat.min.js"></script>

    <!--Owl Carousel js -->
    <script src="~/Areas/Admin/assets/plugins/owl-carousel/owl.carousel.js"></script>
    <script src="~/Areas/Admin/assets/plugins/owl-carousel/owl-main.js"></script>

    <!-- Rightsidebar js -->
    <script src="~/Areas/Admin/assets/plugins/sidebar/sidebar.js"></script>

    <!-- Charts js-->
    <script src="~/Areas/Admin/assets/plugins/chart/chart.bundle.js"></script>
    <script src="~/Areas/Admin/assets/plugins/chart/utils.js"></script>

    <!--Time Counter js-->
    <script src="~/Areas/Admin/assets/plugins/counters/jquery.missofis-countdown.js"></script>
    <script src="~/Areas/Admin/assets/plugins/counters/counter.js"></script>

    <!--Morris  Charts js-->
    <script src="~/Areas/Admin/assets/plugins/morris/raphael-min.js"></script>
    <script src="~/Areas/Admin/assets/plugins/morris/morris.js"></script>

    <!--ckeditor js-->
    <script src="~/Areas/Admin/assets/plugins/tinymce/tinymce.min.js"></script>
    <script src="~/Areas/Admin/assets/js/formeditor.js"></script>

    <!-- Custom js-->
    <script src="~/Areas/Admin/assets/js/custom.js"></script>
}